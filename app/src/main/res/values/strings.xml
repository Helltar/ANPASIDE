    <string name="tpl.helloworld" translatable="false">
"program %s;
var
    s: string;
    x, y: integer;

begin
    s := \'Hello, Android!\';
    x := (GetWidth - GetStringWidth(s)) / 2;
    y := (GetHeight - GetStringHeight(s)) / 2;

    DrawText(s, x, y);
    Repaint;

    Delay(9000);
end."
    </string>

